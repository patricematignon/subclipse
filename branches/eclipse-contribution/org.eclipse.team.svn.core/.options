# Debugging options for the org.eclipse.team.svn.core plugin.

# Turn on debugging for the plugin.
org.eclipse.team.svn.core/debug=true

# Shows when meta-files are modified by a 3rd party
org.eclipse.team.svn.core/metafiles=true

# Shows stream debugging information
org.eclipse.team.svn.core/threading=false

# 
org.eclipse.team.svn.core/status=true