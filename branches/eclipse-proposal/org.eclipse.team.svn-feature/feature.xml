<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.team.svn"
      label="Subversion Team Provider"
      version="3.2.0.qualifier"
      provider-name="Subclipse"
      plugin="org.eclipse.team.svn.core"
      colocation-affinity="org.eclipse.team.svn">

   <description url="http://subclipse.tigris.org/">
      %description
   </description>

   <copyright url="http://subclipse.tigris.org/">
      http://subclipse.tigris.org/
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://subclipse.tigris.org/proposal"/>
      <discovery label="%updateSiteName" url="http://subclipse.tigris.org/proposal"/>
   </url>

   <includes
         id="org.eclipse.subversion.client"
         version="0.0.0"/>

   <requires>
      <import plugin="org.eclipse.subversion.client" version="1.3.0" match="equivalent"/>
      <import plugin="org.eclipse.core.resources" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.team.core" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.osgi"/>
      <import plugin="org.eclipse.compare"/>
      <import plugin="org.eclipse.help"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.team.ui" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.console"/>
   </requires>

   <plugin
         id="org.eclipse.team.svn.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.team.svn.doc"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

   <plugin
         id="org.eclipse.team.svn.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
