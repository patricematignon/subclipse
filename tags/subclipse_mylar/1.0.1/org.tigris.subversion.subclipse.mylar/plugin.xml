<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

  <extension point="org.eclipse.mylar.team.changeSets">
    <activeChangeSetProvider class="org.tigris.subversion.subclipse.mylar.SubclipseTeamChangeSetProvider"/>
  </extension>
   
  <extension point="org.eclipse.core.runtime.adapters">
    <factory adaptableType="org.tigris.subversion.subclipse.core.history.LogEntry"
             class="org.tigris.subversion.subclipse.mylar.SubclipseLinkedTaskInfoAdapterFactory">
      <adapter type="org.eclipse.mylar.tasks.core.ILinkedTaskInfo"/>
    </factory>
    <factory adaptableType="org.tigris.subversion.subclipse.ui.subscriber.SVNChangeSetCollector$SVNCheckedInChangeSet"
             class="org.tigris.subversion.subclipse.mylar.SubclipseLinkedTaskInfoAdapterFactory">
      <adapter type="org.eclipse.mylar.tasks.core.ILinkedTaskInfo"/>
    </factory>
  </extension>
    
</plugin>
